doctype html
html(lang="zh-Hans")
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(http-equiv='X-UA-Compatible' content='ie=edge')
    link(href='assets/styles.css' rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Noto+Serif+TC:400,700,900&display=swap' rel='stylesheet')
    
    title Map

  -
    var resourcesPath = ''
    var imagesPath = resourcesPath + 'images/'
    var iconsPath = resourcesPath + 'icons/'

  include /data/_all
  include /includes/_functions


  block components
    include /components/_all


  block variables
    - var pageClass = '';


  body(class=pageClass)
    include /includes/_header

    section#content
      block mainContent

    //- include /includes/_footer

    block modals
